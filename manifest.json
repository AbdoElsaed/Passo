{
    "manifest_version": 2,
    "name": "Passo",
    "description": "memorize one key for all ur passwords",
    "version": "1.0",
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "passo.1.0.0"
        }
    },
    "permissions": [
        "clipboardRead",
        "clipboardWrite"
    ],
    "icons": {
        "48": "./images/logo.png"
    },
    "browser_action": {
        "default_icon": "./images/logo.png",
        "default_title": "Passo",
        "default_popup": "popup.html"
    }
}